<?xml version='1.0' encoding='utf-8'?>
    <widget xmlns:gap="http://phonegap.com/ns/1.0" 
    xmlns="http://www.w3.org/ns/widgets" 
    id="com.ebusindia.bus" 
    version="2.0.0">

        <name>demo dt</name>
        <description>
            Online volvo bus booking | AC Volvo Bus Booking Online Services
        </description>
        <author email="info@ebusindia.in"
        href="https://www.ebusindia.in">
           Orangespike  team
        </author>
    
    	
    	 <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    	
		   <plugin name="cordova-plugin-device" source="npm" spec="~1.1.1" />
  	       <plugin name="phonegap-plugin-push" spec="2.0.0" />
  		
    	 <platform name="android">
		 <resource-file src="google-services.json" target="app/google-services.json" />
        <icon density="ldpi" src="res/icon/android/icon-36-ldpi.png" />
        <icon density="mdpi" src="res/icon/android/icon-48-mdpi.png" />
        <icon density="hdpi" src="res/icon/android/icon-72-hdpi.png" />
        <icon density="xhdpi" src="res/icon/android/icon-96-xhdpi.png" />
		
      
    </platform>
	<splash src="splash.png" />
	  <splash qualifier="ldpi" platform="android" src="res/screen/android/screen-ldpi-landscape.png" />
        <splash qualifier="mdpi" platform="android" src="res/screen/android/screen-mdpi-landscape.png" />
        <splash qualifier="xhdpi" platform="android" src="res/screen/android/screen-xhdpi-landscape.png" />
        <splash qualifier="hdpi" platform="android" src="res/screen/android/screen-hdpi-portrait.png" />
        <splash qualifier="ldpi" platform="android" src="res/screen/android/screen-ldpi-portrait.png" />
        <splash qualifier="mdpi" platform="android"  src="res/screen/android/screen-mdpi-portrait.png" />
        <splash qualifier="xhdpi" platform="android" src="res/screen/android/screen-xhdpi-portrait.png" />
     <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="SplashMaintainAspectRatio" value="true" />
	     <preference name='phonegap-version' value='cli-7.0.1' />
             <preference name='pgb-builder-version' value='1' />
			 
				
                
                
     <plugin name="cordova-plugin-whitelist" version="1"/>
        

 <content src="index.html" />
    	<access origin="*" />
	</widget>
